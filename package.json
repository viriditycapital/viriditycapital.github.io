{
  "name": "react-webpack-sass-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for Webpack React projects with more features",
  "main": "src/index.js",
  "scripts": {
    "start": "webpack serve --open --config webpack.dev.js",
    "deploy": "npm run build && gh-pages -d build",
    "build": "webpack --config webpack.prod.js",
    "lint": "eslint --ext .js src/",
    "lint-fix": "eslint --fix --ext .js src/"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viriditycapital/react-webpack-sass-boilerplate.git"
  },
  "author": "viriditycapital",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/viriditycapital/react-webpack-sass-boilerplate/issues"
  },
  "homepage": "https://github.com/viriditycapital/react-webpack-sass-boilerplate#readme",
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/node": "^7.13.13",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.21.4",
    "@types/node": "^16.18.3",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "css-loader": "^5.2.4",
    "file-loader": "^6.2.0",
    "gh-pages": "^3.2.3",
    "html-entities": "^2.3.3",
    "html-webpack-plugin": "^5.3.1",
    "react-router-dom": "^6.10.0",
    "sass": "^1.62.0",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^4.2.0"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/material": "^5.12.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
